{
  "manifest_version": 3,
  "name": "Dictionary Pronunciation Clicker",
  "description": "Click pronunciation button in `dictionary.com` via keyboard.",
  "version": "1.0.0",
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+B"
      }
    }
  }
}
